%% QUICK MAFFS
%\DeclareMathOperator{\del}{\partial}
\newcommand{\cpr}[3]{\epsilon^{#1}_{\ #2#3}}
\newcommand{\del}{\partial}
\renewcommand{\real}{\mathfrak{Re}}
\renewcommand{\imaginary}{\mathfrak{Im}}
\newcommand{\varvec}[1]{\underline{#1}}
\renewcommand{\vec}[1]{\mathbf{#1}}
\newcommand{\ver}[1]{\hat{\mathbf{#1}}}
